    "def evaluate_accuracy(data_iter,net):\n",
    "    acc_sum = 0\n",
    "    n = 0\n",
    "\n",
    "    net.eval()\n",
    "    for X,y in data_iter:\n",
    "        X = X.to(device)\n",
    "        y = y.to(device)\n",
    "        acc_sum += (net(X).argmax(dim = 1) == y).float().sum().item()\n",
    "\n",
    "        n += y.shape[0]\n",
    "    return acc_sum/n"
   ],
   "id": "56e974b68533143b",
   "outputs": [],
