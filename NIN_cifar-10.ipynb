   "cell_type": "code",
   "source": [
    "def nin_block(in_channel, out_channel, kernel_size, stride, padding):\n",
    "    blk = nn.Sequential(nn.Conv2d(in_channel,out_channel,kernel_size,stride,padding),\n",
    "                        nn.ReLU(),\n",
    "                        nn.Conv2d(out_channel,out_channel,kernel_size=1),\n",
    "                        nn.ReLU(),\n",
    "                        nn.Conv2d(out_channel,out_channel,kernel_size=1),\n",
    "                        nn.ReLU())\n",
    "    return blk\n"
   ],
   "id": "abe0efcabb387094",
   "outputs": [],
   "execution_count": 13
